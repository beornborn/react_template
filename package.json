{
  "name": "react_template",
  "version": "0.0.1",
  "private": true,
  "main": "index.js",
  "repository": {
    "url": "git@github.com:beornborn/react_template.git",
    "type": "git"
  },
  "scripts": {
    "test": "./node_modules/.bin/nightwatch",
    "local": "webpack-dev-server --config webpack.local.config.js",
    "prod": "webpack -p --config webpack.prod.config.js --progress --colors",
    "deploy": "npm run prod && surge -p dist",
    "flow_install": "flow-typed install",
    "flow": "./node_modules/.bin/flow",
    "eslint": "eslint --format 'node_modules/eslint-friendly-formatter' ./app",
    "check": "npm run flow && npm run eslint"
  },
  "dependencies": {
    "immutability-helper": "^2.1.1",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "prop-types": "^15.5.0",
    "react": "^15.5.0",
    "react-dom": "^15.5.0",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.2",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.1",
    "redux-form": "^6.7.0",
    "redux-logger": "^2.8.1",
    "redux-saga": "0.15.4",
    "styled-components": "^1.4.5",
    "surge": "^0.18.0",
    "whatwg-fetch": "^2.0.2"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
